<table>
  <tr>
    <th>Codigo</th>
    <th>Serie</th>
    <th>Sistema operativo</th>
    <th>Nombre</th>
    <th>Fecha lanzamiento</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @modelos.each do |modelo| %>
  <tr>
    <td><%= modelo.codigo %></td>
    <td><%= modelo.serie.nombre %></td>
    <td><%= modelo.sistema_operativo.nombre %></td>
    <td><%= modelo.nombre %></td>
    <td><%= modelo.fecha_lanzamiento %></td>
    <td><%= link_to 'Show', modelo %></td>
    <td><%= link_to 'Edit', edit_modelo_path(modelo) %></td>
    <td><%= link_to 'Destroy', modelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Modelo', new_modelo_path %>